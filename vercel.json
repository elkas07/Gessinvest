{
  "version": 2,
  "builds": [
    {
      "src": "public/utilisateur.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/admin.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "public/utilisateur.html"
    },
    {
      "src": "/admin",
      "dest": "public/admin.html"
    },
    {
      "src": "/style.css",
      "dest": "public/style.css"
    },
    {
      "src": "/admin.css",
      "dest": "public/admin.css"
    },
    {
      "src": "/scripts/(.*)",
      "dest": "scripts/$1"
    }
  ]
}